end
if (Value) == "Royal Red" then
local args = {
    [1] = 66,
    [2] = "Chair Skins",
    [3] = "Winter"
}

game:GetService("ReplicatedStorage").RemoteFunction:InvokeServer(unpack(args))
end
if (Value) == "Knife Antlers" then
local args = {
    [1] = 66,
    [2] = "Accessories",
    [3] = "Knife Antlers"
}

game:GetService("ReplicatedStorage").RemoteFunction:InvokeServer(unpack(args))
end
end)
        else
        		game:GetService("StarterGui"):SetCore("SendNotification", { 
			Title = "DreadzHub";
			Text = "Wrong Key L Bozo!";
			Icon = "rbxthumb://type=Asset&id=11002973134&w=150&h=150"})
		Duration = 3;
    end
end)
--Config
local Tab4 = Window:NewTab("Config")
local Section = Tab4:NewSection("Config:")
Section:NewKeybind("Toggle UI", "Toggle The UI With Key On Your Keyboard!", Enum.KeyCode.E, function()
	Library:ToggleUI()
		game:GetService("StarterGui"):SetCore("SendNotification", { 
			Title = "DreadzHub";
			Text = "Toggled UI!";
			Icon = "rbxthumb://type=Asset&id=11002973134&w=150&h=150"})
		Duration = 3;
end)
