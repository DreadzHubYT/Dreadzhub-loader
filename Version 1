    location1 = Vector3.new(623.0762329101562, -7476.17822265625, 59299.75390625),
    location2 = Vector3.new(616.882568359375, -7476.17822265625, 59295.16015625),
location3 = Vector3.new(608.876220703125, -7476.17822265625, 59287.4296875),-- your desired position
}
 
local tween_s = game:GetService('TweenService')
local tweeninfo = TweenInfo.new(1,Enum.EasingStyle.Linear)
 
local lp = game.Players.LocalPlayer
 
function bypass_teleport(v)
    if lp.Character and 
    lp.Character:FindFirstChild('HumanoidRootPart') then
        local cf = CFrame.new(v)
        local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
        a:Play()
        -- a.Completed:Wait()
        -- print('Teleporting Done!')
    end
end
 
bypass_teleport(teleport_table.location1)
wait(1)
bypass_teleport(teleport_table.location2)
wait(1)
bypass_teleport(teleport_table.location3)
wait(0.1)
end
    else
--Main
